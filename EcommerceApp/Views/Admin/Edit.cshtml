@model EcommerceApp.Models.AspNetUser
@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Edit</h2>

<form action="/Admin/EditOk/@Model.Id" method="post">
    Name:
    <input type="text" name="name" value="@Model.UserName" /><br />
    Password:
    <input type="password" name="pt" value="@Model.PasswordHash" /><br />
    <input type="submit" name="submit" value="Add" />
</form>